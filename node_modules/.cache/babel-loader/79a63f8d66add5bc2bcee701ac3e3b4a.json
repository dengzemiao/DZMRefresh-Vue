ntainer-refresh\"\n      :isMoreData=\"isMoreData\"\n      :isLoading=\"isLoading\"\n      @loading=\"onLoading\"\n    >\n      <!-- 显示内容 -->\n      <div v-for=\"item in dataSource\" :key=\"item\">{{ item }}</div>\n    </Refresh>\n\n    <!-- 滚动容器二 -->\n    <Refresh class=\"container-refresh\">\n      <!-- 显示内容 -->\n      <div v-for=\"item in dataSource\" :key=\"item\">{{ item }}</div>\n      <!-- 自定义 footer -->\n      <!-- <template slot=\"footer\" slot-scope=\"props\">\n        {{ props }}\n      </template> -->\n      <!-- <template slot=\"footer\" slot-scope=\"props\">\n        <div :style=\"props.footerStyle\">自定义底部</div>\n      </template> -->\n    </Refresh>\n\n  </div>\n</template>\n\n<script>\n// 导入\nimport Refresh from '@/components/Refresh'\nexport default {\n  components: {\n    Refresh\n  },\n  data () {\n    return {\n      // 有数据\n      isMoreData: true,\n      // 加载状态\n      isLoading: false,\n      // 测试数据源\n      dataSource: [1, 2, 3, 4]\n    }\n  },\n  methods: {\n    // 加载中\n    onLoading () {\n      // 加载中\n      this.isLoading = true\n      // 模拟请求\n      setTimeout(() => {\n        // 显示数据\n        this.dataSource = this.dataSource.concat([11, 12, 13, 14, 15, 16])\n        // 没有更多数据\n        this.isMoreData = false\n        // 取消加载\n        this.isLoading = false\n      }, 2000);\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.refresh-view {\n  margin-top: 20px;\n  height: 200px;\n  background-color: greenyellow;\n}\n</style>"],"sourceRoot":"src/views"}]}